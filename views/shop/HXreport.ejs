<% var motor_rpm = (product.engine_high_idle-50)*product.pump_displacement/product.motor_displacement; // 모터 속도
  var reduction_rv = motor_rpm/product.reduction_gear_ratio; // 감속기 속도
  var travel_speed_result = product.No_teeth * product.link_pitch * reduction_rv * product.travel_eff * 60 / (2*10**6)// 주행 속도
%>
<% var GrossWeight = product.operating_weight + 65 // 운전중량 %>
<% 
var ground_Length = product.tumbler_distance+0.35*(product.track_length-product.tumbler_distance);
var ground_pressure = (GrossWeight / (2 * product.shoe_width/10 * ground_Length/10));
var ground_pressure_woqc = ((GrossWeight - product.quick_coupler_weight) / (2 * product.shoe_width/10 * ground_Length/10));
%> 
<% 
var motor_oil_flow = (product.engine_high_idle - 50) * product.pump_displacement / 1000;
var motor_speed = motor_oil_flow * 1000 / product.motor_volume;
var input_speed = motor_speed;
var output_speed = input_speed/product.gear_ratio_1;
var gear_ratio_2 = product.swing_bearing/product.pinion_gear;
var swing_speed = (output_speed/gear_ratio_2)*product.swing_effeciency;
%>
<% 
var parking_force = product.brake_torque * product.reduction_gear_ratio * 4 / product.sprocket_PCD; // Prking 력
var traction_force = product.relief_valve_pressure * product.motor_displacement / (200 * Math.PI) * product.reduction_gear_ratio * 4 / product.sprocket_PCD;  // 구동력
var slippery_horizontal = GrossWeight/1000 * Math.sin(Math.PI/180 * product.greadability_ref);  // 사면을 내려가는 힘
var slippery_vertical = GrossWeight/1000 * Math.cos(Math.PI/180 * product.greadability_ref);  // 사면에 수직방향 힘
var friction = slippery_vertical * 0.15;  // 사면 마찰력
var slippery_grade = slippery_horizontal - friction;  // 사면을 올라가는 힘
var torque_required = (slippery_grade * product.sprocket_PCD)/(4 * product.reduction_gear_ratio) * product.reduction_ratio; // 필요 제동 토크
var angle_greadable = (Math.asin(2*product.brake_torque*product.reduction_gear_ratio / (product.sprocket_PCD / 2*product.reduction_ratio * product.greadability_ref)/ (1+0.15**2)**0.5)-Math.atan(-0.15/1))/(Math.PI / 180);  // 최대 등판 각도
var torque_ratio = product.brake_torque / torque_required;
%>
<%
    var bkt_creep_angle = Math.asin(product.Front_Creep/product.digging_reach)*180/Math.PI;          

    %>

