
    
<!-- ===============================================-->
<!--    제원표    -->
<!-- ===============================================-->

  <div class="pages" id="mainSpec">
    <table style="height: 270mm">
      <thead>
        <tr>
          <th colspan="6" height="50px">건설기계 (타이어식 등) 제원표</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td colspan="2" width="32%">형식승인(신고)자</td>
          <td colspan="4">현대건설기계</td>
        </tr>
        <tr>
          <td colspan="2">건설기계명</td>
          <td width="15%">굴착기</td>
          <td colspan="2" width="30%">제작회사(제작국)</td>
          <td>현대건설기계 <br> (대한민국) </td>
        </tr>
        <tr>
          <td colspan="2">형식</td>
          <td> <%= product.model_name %></td>
          <td colspan="2">규격</td>
          <td><%= product.machine_grade %></td>
        </tr>
        <tr>
          <td colspan="2">자체중량 (㎏)</td>
          <td><%= product.operating_weight %></td>
          <td colspan="2">변속기</td>
          <td><%= product.gearbox %></td>
        </tr>
        <tr>
          <td colspan="2">최고속도 (㎞/hr)</td>
          <td id=""><%= travel_speed_result.toFixed(2) %> </td>
          <td rowspan="6" width="4%" class="vertical_cell">엔진</td>
          <td>형식</td>
          <td id=""><%= product.engine_name %> </td>
        </tr>
        <tr>
          <td colspan="2">등판능력 (무부하)</td>
          <td id=""><%= product.greadability_ref %> </td>
          <td>출력(정격출력)</td>
          <td id=""><%= product.engine_power %></td>
        </tr>
        <tr>
          <td rowspan="4" width="4%" class="vertical_cell">외관</td>
          <td>길이 (㎜)</td>
          <td id=""><%= product.overall_length %></td>
          <td>최대토크(㎏ㆍm/rpm)</td>
          <td id=""><%= product.engine_torque %></td>
        </tr>
        <tr>
          <td>너비 (㎜)</td>
          <td id=""><%= product.overall_width %> </td>
          <td>기통수</td>
          <td id=""><%= product.engine_cylinder %>기통</td>
        </tr>
        <tr>
          <td>높이 (㎜)</td>
          <td id=""><%= product.overall_height %> </td>
          <td>연료종류</td>
          <td>디젤 </td>
        </tr>
        <tr>
          <td>최저지상고 (㎜)</td>
          <td id=""><%= product.ground_clearance %> </td>
          <td>제작회사</td>
          <td id=""><%= product.engine_supplier %> </td>
        </tr>
        <tr>
          <td rowspan="5" class="vertical_cell">무한궤도식등</td>
          <td>슈폭 (㎜)</td>
          <td id=""><%= product.shoe_width %> </td>
          <td rowspan="5" class="vertical_cell">드럼등기타방식</td>
          <td>드럼치수(㎜× ㎜)</td>
          <td id="drum_size">-</td>
        </tr>
        <tr>
          <td>트랙높이</td>
          <td id=""><%= product.track_height %> </td>
          <td>타이어치수</td>
          <td id="tire_radius">-</td>
        </tr>
        <tr>
          <td>트랙중심간거리 (㎜)</td>
          <td id=""><%= product.track_gap %> </td>
          <td>축간거리 (㎜)</td>
          <td id="wheel_base">-</td>
        </tr>
        <tr>
          <td>텀블러중심간거리 (㎜)</td>
          <td id=""><%= product.tumbler_distance %> </td>
          <td>최소회전반경 (㎜)</td>
          <td id="min_turning_raduis">-</td>
        </tr>
        <tr>
          <td>접지압(㎏/㎠)</td>
          <td id=""><%= ground_pressure.toFixed(2) %> </td>
          <td>제동거리 (m/ ㎞/hr)</td>
          <td id="braking_distance">-</td>
        </tr>
        <tr>
          <td colspan="6">작 업 장 치</td>
        </tr>
        <tr>
          <td colspan="2">버킷용량(평적) (㎥)</td>
          <td id=""><%= product.bucket_struck %> </td>
          <td colspan="2">버킷용량(산적) (㎥)</td>
          <td id=""><%= product.bucket_heap %> </td>
        </tr>
        <tr>
          <td colspan="2">선회속도 (rpm)</td>
          <td id=""><%= swing_rev %></td>
          <td colspan="2">암의길이 (㎜)</td>
          <td id=""><%= product.arm_length %> </td>
        </tr>
        <tr>
          <td colspan="2">붐의길이 (㎜)</td>
          <td id=""><%= product.boom_length %></td>
          <td colspan="2">최대굴착반지름 (㎜)</td>
          <td id=""><%= product.digging_reach %> </td>
        </tr>
        <tr>
          <td colspan="2">최대굴착깊이 (㎜)</td>
          <td id=""><%= product.digging_depth %></td>
          <td colspan="2">최대덤프높이 (㎜)</td>
          <td id=""><%= product.loading_height %> </td>
        </tr>
        <tr>
          <td colspan="2">배토판너비x높이 (㎜)</td>
          <td id=""><%= product.dozer_size %></td>
          <td colspan="2"> </td>
          <td> </td>
        </tr>
        <tr>
          <td colspan="2">퀵 커플러 (모델명 / 중량(kg))</td>
          <td colspan="2" id=""><%= product.quick_coupler %></td>
          <td id=""><%= product.quick_coupler_weight %></td>
          <td></td>
        </tr>
        <tr>
          <td colspan="2">비고</td>
          <td colspan="4" id=""><%= product.ETC %></td>
        </tr>

        <tr>
          <td colspan="2">형식승인연월일</td>
          <td> </td>
          <td colspan="2">형식승인번호</td>
          <td> </td>
        </tr>
      </tbody>
    </table>

  </div>