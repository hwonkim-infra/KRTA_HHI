                                <td><%= product.travel_ring_gear_R1 %></td>
                            </tr>
                            <tr>
                                <td>주행선기어2</td>
                                <td>S2</td>
                                <td><%= product.travel_sun_gear_S2 %></td>
                                <td rowspan="2">$$i_2 = {(S_2+R_2) \over S_2}$$</td>
                                <td rowspan="2">$${(<%= product.travel_sun_gear_S2 %>+<%= product.travel_ring_gear_R2 %>) \over <%= product.travel_sun_gear_S2 %>}$$</td>
