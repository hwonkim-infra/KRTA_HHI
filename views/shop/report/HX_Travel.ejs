                            <tr>
                                <td class="item-headers" colspan="3">주행감속비</td>
                                <td>$$I_{total} = i_1 \times i_2 -1 $$</td>
                                <td>$$<%= travel_reduc_1 %> \times <%= travel_reduc_2 %> -1 $$</td>
                                <td>$$<%= travel_reduc %>$$</td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </table>
    </div>


