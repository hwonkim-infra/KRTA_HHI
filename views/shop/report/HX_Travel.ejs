<% 
    var gross_weight = product.operating_weight + 65;
 %>

<% 
    var travel_reduc_1 = (1 + product.travel_ring_gear_R1 / product.travel_sun_gear_S1).toFixed(2);
    var travel_reduc_2 = (1 + product.travel_ring_gear_R2 / product.travel_sun_gear_S2).toFixed(2);
    var travel_reduc = (travel_reduc_1 * travel_reduc_2 - 1).toFixed(2);

 %> 
<%  // 주행 속도
    var TM_rev_1 = (product.motor_displacement_travel * product.TM_mv/(product.TM_flow_1 * product.travel_reduc)*1000).toFixed(2);
    var TM_rev_2 = (product.motor_displacement_travel * product.TM_mv/(product.TM_flow_2 * product.travel_reduc)*1000).toFixed(2);
    var travel_speed_1 = (TM_rev_1 * 2 * Math.PI*product.sprocket_radius * 60 / 10**6).toFixed(2);
    var travel_speed_2 = (TM_rev_2 * 2 * Math.PI*product.sprocket_radius * 60 / 10**6).toFixed(2);  
    var travel_speed_out = Math.max(travel_speed_1, travel_speed_2);

 %> 
<% 
    var TM_1 = (( product.travel_pump_pressure  * product.TM_flow_1  / (200* Math.PI)) *  travel_reduc  *  product.TM_mt).toFixed(2);
    var TM_2 = (( product.travel_pump_pressure  * product.TM_flow_2  / (200* Math.PI)) *  travel_reduc  *  product.TM_mt).toFixed(2);
    var Traction_Sprocket = (2 * TM_2 * 1000 / product.sprocket_radius) * product.TM_r;
    var ground_traction = product.surface_drag * 54850;
 %> 

<% 
    var TS = ((2 * TM_1 * 1000 / product.sprocket_radius) * product.TM_r).toFixed(0);   // 스프로켓 최대 구동력
    var AF = (product.surface_drag * (product.operating_weight + 65)).toFixed(2);   // 점착력
    var DP = Math.min(TS, AF);  // 견인력
    var traction_downward = (product.sprocket_radius / 1000 * gross_weight * Math.sin(16.7/180*Math.PI)).toFixed(2);  // 스프로켓 구동력

%> 

<% 
    var theta_1 = (Math.atan(product.surface_drag)*(180/Math.PI)).toFixed(1);   // 경사지 지탱 각도
    var theta_2 = ((180/Math.PI) * Math.asin( (DP  - product.travel_drag * gross_weight) / gross_weight)).toFixed(1); // 구동 등판 각도
    var theta_3 = product.greadability_ref; // 기준 등판각
 %> 

    <!-- ===============================================-->
    <!--    주행 능력    -->
    <!-- ===============================================-->
