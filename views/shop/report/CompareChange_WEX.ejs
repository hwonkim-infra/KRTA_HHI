<% var GrossWeight = product.operating_weight + 65 // 운전중량 %>
<% 
var ground_Length = product.tumbler_distance+0.35*(product.track_length-product.tumbler_distance);
var ground_pressure = (GrossWeight / (2 * product.shoe_width/10 * ground_Length/10)).toFixed(2);
%> 

<% var GrossWeight_origin = origin.operating_weight + 65 // 운전중량 %>
<% 
var ground_Length_origin = origin.tumbler_distance+0.35*(origin.track_length-origin.tumbler_distance);
var ground_pressure_origin = (GrossWeight_origin / (2 * origin.shoe_width/10 * ground_Length_origin/10)).toFixed(2);
%> 




<!-- ===============================================-->
<!--    제원 비교    -->
<!-- ===============================================-->

<div class="pages" id="compareChange">
    <table class="bordertable">
        <tr class="borderheader">
            <td height="30mm">형식 변경에 따른 제원 비교</td>
        </tr>

        <tr>
            <td>
                <table class="innertable" height="" width="80%">

                    <tr height="70">
                        <th width="25%">제원 항목</th>
                        <th>변경 전</th>
                        <th>변경 후</th>
                    </tr>
                    </thead>
                    <tbody>
                        <% if(origin.boom_length !== product.boom_length) { %> <tr height="50">
                            <td>붐</td>
                            <td><%= origin.boom_length %></td>
                            <td><%= product.boom_length %>  </td>
                        </tr><% } %> 
                        <% if(origin.arm_length !== product.arm_length) { %> <tr height="50">
                            <td>암</td>
                            <td><%= origin.arm_length %></td>
                            <td><%= product.arm_length %>  </td>
                        </tr><% } %>
                        <% if(origin.bucket_struck !== product.bucket_struck) { %> <tr height="50">
                            <td>버켓 (평적 / 산적)</td>
                            <td><%= origin.bucket_struck %> / <%= origin.bucket_heap %></td>
                            <td><%= product.bucket_struck %> / <%= product.bucket_heap %> </td>
                        </tr><% } %>
                        <% if(origin.quick_coupler !== product.quick_coupler) { %> <tr height="50">
                            <td>퀵 커플러</td>
                            <td><%= origin.quick_coupler %></td>
                            <td><%= product.quick_coupler %>  </td>
                        </tr><% } %>
                        <% if(origin.operating_weight !== product.operating_weight) { %> <tr height="50">
                            <td>자체중량</td>
                            <td><%= origin.operating_weight %></td>
                            <td><%= product.operating_weight %>  </td>
                        </tr><% } %>
                        <% if(origin.overall_length !== product.overall_length) { %> <tr height="50">
                            <td>길이</td>
                            <td><%= origin.overall_length %></td>
                            <td><%= product.overall_length %>  </td>
                        </tr><% } %>
                        <% if(origin.overall_width !== product.overall_width) { %> <tr height="50">

                            <td>너비</td>
                            <td><%= origin.overall_width %></td>
                            <td><%= product.overall_width %>  </td>
                        </tr><% } %>
                        <% if(origin.overall_height !== product.overall_height) { %> <tr height="50">

                            <td>높이</td>
                            <td><%= origin.overall_height %></td>
                            <td><%= product.overall_height %>  </td>
                        </tr><% } %>
                        <% if(origin.digging_reach !== product.digging_reach) { %> <tr height="50">

                            <td>최대굴착반지름</td>
                            <td><%= origin.digging_reach %></td>
                            <td><%= product.digging_reach %>  </td>
                        </tr><% } %>
                        <% if(origin.digging_depth !== product.digging_depth) { %> <tr height="50">

                            <td>최대굴착깊이</td>
                            <td><%= origin.digging_depth %></td>
                            <td><%= product.digging_depth %>  </td>
                        </tr><% } %>
                        <% if(origin.loading_height !== product.loading_height) { %> <tr height="50">

                            <td>최대덤프높이</td>
                            <td><%= origin.loading_height %></td>
                            <td><%= product.loading_height %>  </td>
                        </tr><% } %>
                        <% if(origin.dozer_size !== product.dozer_size) { %> <tr height="50">

                            <td>배토판</td>
                            <td><%= origin.dozer_size %></td>
                            <td><%= product.dozer_size %>  </td>
                        </tr><% } %>
                        <% if(origin.ground_clearance !== product.ground_clearance) { %> <tr height="50">

                            <td>최저지상고</td>
                            <td><%= origin.ground_clearance %></td>
                            <td><%= product.ground_clearance %>  </td>
                        </tr><% } %>

                        
                    </tbody>

                </table>
            </td>
        </tr>
    </table>
</div>