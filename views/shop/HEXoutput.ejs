<% var GrossWeight = product.operating_weight + 65 // 운전중량 %>
<% 
var ground_Length = product.tumbler_distance+0.35*(product.track_length-product.tumbler_distance);
var ground_pressure = (GrossWeight / (2 * product.shoe_width/10 * ground_Length/10));
var ground_pressure_woqc = ((GrossWeight - product.quick_coupler_weight) / (2 * product.shoe_width/10 * ground_Length/10));
%> 

<%
    var bkt_creep_angle = Math.asin((product.bucket_creep_before - product.bucket_creep_after)/product.digging_reach)*180/Math.PI;          

    %>
    
<% 
  var planet_reduct_1 = 1+product.planetary_ring_gear_C1/product.planetary_sun_gear_A1;
  var planet_reduct_2 = 1+product.planetary_ring_gear_C2/product.planetary_sun_gear_A2;
  var planet_reduct_tot = (planet_reduct_1 * planet_reduct_2).toFixed(2)
  var swing_reduction_rev =  (product.swing_pump_flow*product.swing_motor_eff/product.motor_displacement_swing*1000).toFixed(2)
  var swing_rev = (swing_reduction_rev/planet_reduct_tot*product.gear_pinion/product.gear_swing_bearing).toFixed(1);
%>    
 <% 
    var travel_reduc_1 = (1 + product.travel_ring_gear_R1 / product.travel_sun_gear_S1).toFixed(2);
    var travel_reduc_2 = (1 + product.travel_ring_gear_R2 / product.travel_sun_gear_S2).toFixed(2);
    var travel_reduc = (travel_reduc_1 * travel_reduc_2 - 1).toFixed(2);

 %> 
<%  // 주행 속도
    var TM_rev_1 = (product.motor_displacement_travel * product.TM_mv/(product.TM_flow_1 * travel_reduc)*1000).toFixed(2);
    var TM_rev_2 = (product.motor_displacement_travel * product.TM_mv/(product.TM_flow_2 * travel_reduc)*1000).toFixed(2);
    var travel_speed_1 = (TM_rev_1 * 2 * Math.PI*product.sprocket_radius * 60 / 10**6).toFixed(2);
    var travel_speed_2 = (TM_rev_2 * 2 * Math.PI*product.sprocket_radius * 60 / 10**6).toFixed(2);  
    var travel_speed_out = Math.max(travel_speed_1, travel_speed_2);
 %>  

<% 
    var TM_1 = (( product.travel_pump_pressure  * product.TM_flow_1  / (200* Math.PI)) *  travel_reduc  *  product.TM_mt).toFixed(2);
    var TM_2 = (( product.travel_pump_pressure  * product.TM_flow_2  / (200* Math.PI)) *  travel_reduc  *  product.TM_mt).toFixed(2);
    var Traction_Sprocket = (2 * TM_2 * 1000 / product.sprocket_radius) * product.TM_r;
    var ground_traction = product.surface_drag * 54850;
 %> 

