<%- include('includes/head.ejs') %>

</head>
<body>
  <%- include('includes/navigation.ejs') %>



  <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
    <div class=" justify-content-between flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">

      <div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Code</th>
              <th scope="col">Model Name</th>
              <th scope="col">Engine</th>
              <th scope="col">Suffix</th>
              <th scope="col">Power (kW)</th>
              <th scope="col">Emission Certi</th>
              <th scope="col">소음 기준</th>
              <th scope="col">EMC 인증서</th>

            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < data.length; i++) { %>
            <tr>
              <td><%= data[i].Code %></td>
              <td><%= data[i].ModelName %></td>
              <td><%= data[i].Engine %></td>
              <td><%= data[i].Suffix %></td>
              <td><%= data[i].Power %></td>
              <td><%= data[i].EngineCerti %></td>
              <td><%= data[i].Noise %></td>
              <td><%= data[i].EMC %></td>
            </tr>
            <% } %>

          </tbody>
        </table>
      </div>
    </div>

  </main>

  <%- include('includes/end.ejs') %>